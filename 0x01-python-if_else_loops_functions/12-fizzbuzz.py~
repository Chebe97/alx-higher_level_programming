#!/usr/bin/python3
#A function that prints numbers 1 to 100
#For multiples of three print Fizz
#For multiples of five print Buzz
#For multiples of three and five print FizzBuzz

def fizzbuzz():
    for number in range(1, 101):
        if number % 15 == 0:
            print("FizzBuzz ", end="")
            continue
        elif number % 3 == 0:
            print("Fizz ", end="")
            continue
        elif number % 5 == 0:
            print("Buzz ", end="")
            continue
        else:
            print("{} ".format(number), end="")
