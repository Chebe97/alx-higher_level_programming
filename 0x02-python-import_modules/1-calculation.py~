#!/usr/bin/python3

if __name__ == "__main__":
    from calculator_1 import add, sub, mul, div
    a = 10
    b = 5

    #addition
    print("{} + {} = {}".format(a, b, add(a,b)))
    #subraction
    print("{} - {} = {}".format(a, b, subtract(a, b)))
    #multiplication
    print("{} * {} = {}".format(a, b, multiply(a, b)))
    #division
    print("{} / {} = {}".format(a,b, divide(a,b)))


    
